<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:typeset="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
  <org.kbsriram.android.example.BitmapTypesetView
      android:id="@+id/margin_demo2_typesetview"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:paddingTop="@dimen/module"
      android:paddingBottom="@dimen/module"
      android:paddingLeft="@dimen/margin"
      android:paddingRight="@dimen/margin"
      typeset:typeColor="#ff333330"
      typeset:typeSize="@dimen/title_text"
      typeset:typeMaximumLineStretch="@dimen/module_x2"
      typeset:typeMaximumGlueExpansionRatio="1.5"/>
</ScrollView>
